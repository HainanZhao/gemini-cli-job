# Gemini CLI Job Configuration
# Copy this file to .env and update with your values

# Google Cloud Project ID (Required)
GOOGLE_CLOUD_PROJECT=your-project-id

# Gemini Model to use (Optional, defaults to gemini-1.5-flash)
GEMINI_MODEL=gemini-1.5-flash

# Job Mode (Optional, defaults to 'p')
JOB_MODE=p

# Enable notifications (Optional, defaults to true)
GEMINI_NOTIFICATION_ENABLED=true

# Opsgenie API Key (Optional, only needed for Opsgenie notifications)
# OPSGENIE_API_KEY=your-opsgenie-api-key

# Debug mode (Optional, set to 'true' to enable debug logging)
# DEBUG=false